{
  "manifest_version": 3,
  "name": "DOI Citation Fetcher",
  "version": "1.1",
  "description": "Fetches AMA-style citations for a given DOI",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["<all_urls>", "https://citation.doi.org/*"]
}
